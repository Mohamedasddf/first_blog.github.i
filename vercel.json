{
  "version": 2,
  "builds": [
    {
      "src": "first_blog/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "first_blog/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "first_blog.settings",
    "DATABASE_URL": "postgres://user:password@hostname:port/dbname" 
  }
}
